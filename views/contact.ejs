<% layout('layouts/boilerplate') %>
    <section class="contacts">
        <div class="contact-container">
            <div class="form-container">
                <form action="" id="contact-form">
                    <div class="form-wrapper">
                        <div class="get-started outline">
                            <h3>GET STARTED TODAY</h3>
                        </div>
                        <div class="form-controler">
                            <div class="first-row ">
                                <label for="name"></label>
                                <input type="text" name="name" id="name" placeholder="First Name*">
                            </div>
                            <div class="first-row ">
                                <label for="lastNamee"></label>
                                <input type="text" name="lastName" id="lastName" placeholder="Last Name">
                            </div>
                            <div class="first-row ">
                                <label for="email"></label>
                                <input type="text" name="email" id="email" placeholder="Email*">
                            </div>
                            <div class="first-row ">
                                <label for="phone"></label>
                                <input type="text" name="phone" id="phone" placeholder="Phone (Optional)">
                            </div>
                        </div>
                        <div class="outline" id="check">
                            <input type="checkbox" id="requestConsultation" name="requestConsultation">
                            <label for="requestConsultation">Request a consultation</label>
                        </div>
                        <div class="get-started outline">
                            <h3>COMPLETE IF REQUESTING CONSULTATION</h3>
                            <p>if requesting a consultation, please complete the information below.</p>
                        </div>
                        <div class="form-controler">
                            <div class="second-row ">
                                <label for="height"></label>
                                <input type="text" name="height" id="height" placeholder="Height">
                            </div>
                            <div class="second-row ">
                                <label for="weight"></label>
                                <input type="text" name="weight" id="weight" placeholder="Weight">
                            </div>
                            <div class="second-row ">
                                <label for="birth_date"></label>
                                <input type="text" name="birth_date" id="birth_date"
                                    placeholder="Date of Birth - dd/mm/yy">
                            </div>
                        </div>
                        <div class="third-row outline">
                            <label for="trainings_per_week"></label>
                            <input type="text" name="trainings_per_week" id="trainings_per_week"
                                placeholder="How many days a week are you currently working out?">
                        </div>
                        <div class="third-row outline">
                            <label for="training_duration"></label>
                            <input type="text" name="training_duration" id="training_duration"
                                placeholder="How long are your typical workouts?">
                        </div>
                        <div class="third-row outline">
                            <label for="weekly_training_goals"></label>
                            <input type="text" name="weekly_training_goals" id="weekly_training_goals"
                                placeholder="How many days realistically would you like to work out?">
                        </div>
                        <div class="third-row outline">
                            <label for="fitness_goals"></label>
                            <input type="text" name="fitness_goals" id="fitness_goals"
                                placeholder="What are your fitness goals?">
                        </div>
                        <div class="third-row outline">
                            <label for="trainig_schedule"></label>
                            <input type="text" name="trainig_schedule" id="trainig_schedule"
                                placeholder="What days and hour do you typically work out?">
                        </div>
                        <div class="third-row outline">
                            <label for="mesage"></label>
                            <textarea name="mesage" id="mesage" cols="3" rows="6" placeholder="Message"></textarea>
                        </div>
                        <div class="third-row outline">
                            <label for="information_source"></label>
                            <input type="text" name="information_source" id="information_source"
                                placeholder="How did you hear about Sault Fitness?">
                        </div>
                        <div class="start-button outline">
                            <button id="btnStart">START NOW</button>
                        </div>
                    </div>
                </form>
                <div class="divider outline">
                    <div class="horizontal-line">
                        <p></p>
                    </div>
                </div>
                <div class="location-details">
                    <div class="location">
                        <div class="location-wrapper">
                            <div class="location-top">
                                <div class="location-header">
                                    <h2>LOCATION</h2>
                                </div>
                            </div>
                            <div class="location-bottom">
                                <div class="bottom-wraper">
                                    <p>102 State Road</p>
                                    <p>North Darmouth MA 02747</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="location location1">
                        <div class="location-wrapper">
                            <div class="location-top">
                                <div class="location-header location-header1">
                                    <h2>MORE DETAILS</h2>
                                </div>
                            </div>
                            <div class="location-bottom">
                                <div class="bottom-wraper bottom-wraper1">
                                    <p>
                                        <a href="info@saultfitness.com">info@gymfitness.com</a>
                                    </p>
                                    <p>
                                        <a href="tel:508-207-6698">508.207.6608</a>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>